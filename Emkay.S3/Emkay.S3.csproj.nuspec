    <id>Emkay.S3</id>
    <version>1.0.0.1</version>
    <authors>Martin Komischke,Jared Dobson</authors>
    <owners>Jared Dobson</owners>
    <licenseUrl>http://opensource.org/licenses/mit-license.php</licenseUrl>
